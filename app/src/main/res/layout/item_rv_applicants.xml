        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:text="Applicant 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            android:id="@+id/item_rv_applicants_tv_applicant_number"
            android:textColor="@color/colorSalamon"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginStart="8dp" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Work Factors"
            android:id="@+id/textView6"
            android:textColor="@color/colorPrimaryText"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/item_rv_applicants_tv_applicant_number"
            android:layout_marginStart="8dp" />

        <android.support.v7.widget.AppCompatCheckedTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            android:text="Confidence"
            android:textColor="@color/colorMonaLisa"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp"
            android:textSize="15sp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            android:id="@+id/appCompatCheckedTextView" />

        <android.support.v7.widget.AppCompatCheckedTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            android:textSize="15sp"
            android:textColor="@color/colorMonaLisa"
            android:text="Compatibility"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/appCompatCheckedTextView"
