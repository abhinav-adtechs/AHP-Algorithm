            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp"
            android:textSize="15sp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView69"
            android:id="@+id/appCompatCheckedTextView9" />

        <android.support.v7.widget.AppCompatCheckedTextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            android:textSize="15sp"
            android:textColor="@color/colorMonaLisa"
            android:text="Technical Knowledge"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/appCompatCheckedTextView9"
            android:id="@+id/appCompatCheckedTextView29" />

        <android.support.v7.widget.AppCompatCheckedTextView
